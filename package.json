{
	"name": "binary",
	"displayName": "Binary",
	"description": "binary",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.binaryPreview"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.binaryPreview",
				"title": "显示二进制预览"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "extension.binaryPreview",
					"group": "navigation"
				}
			],
			"editor/title": [
				{
					"command": "extension.binaryPreview",
					"group": "navigation"
				}
			],
			"editor/title/context": [
				{
					"command": "extension.binaryPreview",
					"group": "navigation"
				}
			],
			"explorer/context": [
				{
					"command": "extension.binaryPreview",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}